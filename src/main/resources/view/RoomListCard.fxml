<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
  <GridPane HBox.hgrow="ALWAYS">
    <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10" prefWidth="150" />
    </columnConstraints>
    <VBox alignment="CENTER" minHeight="105" GridPane.columnIndex="0">
      <padding>
        <Insets bottom="5" left="15" right="5" top="5" />
      </padding>
      <HBox alignment="CENTER" spacing="5">
            <ImageView fx:id="showOccupancy" fitHeight="22.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" translateX="-3.0">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
        <Label fx:id="roomNumber" alignment="CENTER" contentDisplay="CENTER" styleClass="cell_big_label" text="\$first" />
      </HBox>
    </VBox>
      <rowConstraints>
         <RowConstraints />
      </rowConstraints>
  </GridPane>
</HBox>
